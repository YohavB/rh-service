# RushHour Backend Environment Variables
# Copy this file to .env and update with your values

# Database Configuration (External MySQL Service)
# Update these to match your rh-mysql service configuration
DATABASE_URL=jdbc:mysql://localhost:3306/rushhour?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
DB_USERNAME=rushhour_user
DB_PASSWORD=your_secure_password

# Application Configuration
SERVER_PORT=8008

# Logging Configuration
LOGGING_LEVEL_COM_YB_RH=INFO
LOGGING_LEVEL_ORG_SPRINGFRAMEWORK_WEB=WARN
LOGGING_LEVEL_ORG_HIBERNATE_SQL=WARN 